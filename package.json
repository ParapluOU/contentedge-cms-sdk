{
  "name": "@codesocietyou/contentedge-cms-sdk",
  "private": false,
  "version": "0.2.2",
  "description": "A lightweight, framework-agnostic TypeScript client for the ContentEdge headless CMS with API key authentication and optional React Query integration.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "lint": "eslint .",
    "build": "tsdown",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "changeset": "changeset",
    "version:apply": "changeset version",
    "release": "changeset publish"
  },
  "keywords": [
    "content",
    "cms",
    "sdk",
    "typescript",
    "headless-cms",
    "react-query",
    "tanstack"
  ],
  "author": "ContentEdge",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ParapluOU/contentedge-cms-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/ParapluOU/contentedge-cms-sdk/issues"
  },
  "homepage": "https://github.com/ParapluOU/contentedge-cms-sdk#readme",
  "engines": {
    "node": ">=24.13.1",
    "npm": ">=10.0.0"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "dependencies": {
    "axios": "^1.13.3"
  },
  "peerDependencies": {
    "@tanstack/react-query": ">=5.90.0"
  },
  "peerDependenciesMeta": {
    "@tanstack/react-query": {
      "optional": true
    }
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@eslint/js": "^10.0.1",
    "@tanstack/react-query": "^5.90.21",
    "@types/node": "^25.2.3",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.4",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^10.0.0",
    "globals": "^17.3.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "tsdown": "^0.3.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "8.55.1-alpha.4",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  }
}
